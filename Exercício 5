funcao inteiro fibonacci(inteiro n) 

    se n < 0
        Escreva ('Informe somente nÃºmeros maiores ou iguais a 0.');
        retorne nulo;
    senao se n == 0
        retorne 0;
    senao se n == 1
        retorne 1;
    senao
        retorne fibonacci(n - 1) + fibonacci(n - 2);
fimse

