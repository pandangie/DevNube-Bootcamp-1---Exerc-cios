funcao inteiro fibonacci(inteiro n) 

    se n < 0 entao 
        Escreva ('Informe somente nÃºmeros positivos.');
        retorno vazio;
    senao
        se n == 0 
            retorne 0;
        senao 
            se n == 1 
                retorne 1;
            senao 
                retorne fibonacci = fibonacci(n - 1) + fibonacci(n - 2);
            fimse
        fimse
    fimse
fimfuncao

